{
    "rules": {
        "indent": [
            2,
            "tab",
            { "SwitchCase": 1 }
        ],
        "no-unused-vars": 1,
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}